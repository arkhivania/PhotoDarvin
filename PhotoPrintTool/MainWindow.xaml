<Window x:Class="PhotoPrintTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:PhotoPrintTool" WindowStyle="SingleBorderWindow"
        mc:Ignorable="d"
        Title="PrintPhoto" WindowState="Maximized">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <ToolBarTray Grid.Row="0">
            <ToolBar Name="toolBar_Main" BandIndex="0" ToolBarTray.IsLocked="True"/>
            <ToolBar Name="toolBar_Layout" BandIndex="1" ToolBarTray.IsLocked="True"/>
            <ToolBar Name="toolBar_Tools" BandIndex="2" ToolBarTray.IsLocked="True"/>
        </ToolBarTray>
        
        <Grid Grid.Row="1" Name="grid_Sheet">
            
        </Grid>
        
    </Grid>
</Window>
